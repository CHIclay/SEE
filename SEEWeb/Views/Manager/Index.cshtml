@model IEnumerable<Model.Manager>

    @{ 
        ViewBag.Title = "Index";
        Layout = "~/Views/Shared/_LayoutManager.cshtml";
    }
<h2>管理员—主页</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Man_ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Man_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Man_Pas)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Man_Time)
        </th>
        <th>
            @Session["Man_Name"];
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Man_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Man_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Man_Pas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Man_Time)
            </td>
            <td>
                @Html.ActionLink("修改", "Edit", new { id = item.Man_ID })|
                @Html.ActionLink("删除", "Delete", new { id = item.Man_ID })
            </td>

        </tr>
    }
</table>
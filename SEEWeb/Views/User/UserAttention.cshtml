@using Model
@model SEEWeb.ViewModel.UserViewModel
@{
    ViewBag.Title = "UserAttention";
    Layout = null;
}

<div>
    @foreach (Attention p in Model.Attention2)
    {
        <div class="blog-grids">
            <div class="blog-grid-left">
                <a href="@Url.Action("UserIndex","User",new {User_ID=p.User.User_ID})"> <img src="@p.User.User_Img" width="50" height="50" /></a>
             </div>
            <div class="blog-grid-right">

                <h5><a href="@Url.Action("UserIndex","User",new {User_ID=p.User.User_ID})">@p.User.User_Name</a> </h5>
            </div>
            <div class="clearfix"> </div>
        </div>

    }
</div>

